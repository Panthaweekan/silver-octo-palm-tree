{
  "name": "fitjourney-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "FitJourney - Comprehensive Fitness & Health Tracking Platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.2.3",
  "dependencies": {
    "@vercel/speed-insights": "^1.3.1"
  }
}
